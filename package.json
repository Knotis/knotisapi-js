{
    "name": "knotisapi",
    "version": "1.0.2",
    "description": "Javascript library for accessing the Knotis REST API.",
    "main": "dist/index.js",
    "scripts": {
        "build": "babel src --out-dir dist",
        "clean": "rm -rf dist/*",
        "build-dev": "babel src --out-dir dist --source-maps",
        "build-web": "WEBPACK_ENV=web webpack --progress --profile --colors",
        "prepublish": "npm run clean && npm run build",
        "test": "npm run build-dev && intern-client config=tests/intern || true"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/knotis/knotisapi-js.git"
    },
    "keywords": [
        "knotis",
        "client",
        "application",
        "app",
        "javascript",
        "web",
        "mobile",
        "rest",
        "api"
    ],
    "author": "Knotis Inc.",
    "license": "LicenseRef-LICENSE",
    "bugs": {
        "url": "https://github.com/knotis/knotisapi-js/issues"
    },
    "homepage": "https://github.com/knotis/knotisapi-js#readme",
    "devDependencies": {
        "babel": "^6.5.2",
        "babel-cli": "^6.10.1",
        "babel-core": "^6.9.1",
        "babel-eslint": "^6.0.4",
        "babel-loader": "^6.2.4",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-transform-es2015-modules-amd": "^6.8.0",
        "babel-preset-es2015": "^6.9.0",
        "eslint": "^2.11.1",
        "eslint-loader": "^1.3.0",
        "intern": "^3.2.3",
        "webpack": "^1.13.1"
    },
    "dependencies": {
        "librestapi": "^0.2.4"
    }
}
